INSERT INTO QUESTOES VALUES(11,'C03Q01','Qual \u00e9 a finalidade do CSS em rela\u00e7\u00e3o ao desenvolvimento Web?','Definir a estrutura do documento HTML.','Adicionar interatividade ao site.','Controlar a apresenta\u00e7\u00e3o e o estilo do documento HTML.','Criar fun\u00e7\u00f5es de back-end para o site.','C','CSS',1)
INSERT INTO QUESTOES VALUES(12,'C03Q02','Qual \u00e9 a sintaxe correta para selecionar um elemento HTML com a classe "destaque" em CSS?','!destaque','.destaque','<destaque>','"destaque"','B','CSS',1)
