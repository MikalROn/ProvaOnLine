INSERT INTO QUESTOES VALUES(6,'C02Q01','O que \u00e9 Docker?','Uma linguagem de programa\u00e7\u00e3o','Uma plataforma de servi\u00e7o em nuvem','Uma ferramenta de virtualiza\u00e7\u00e3o de aplicativos em cont\u00eaineres','Um sistema operacional','C','Docker',1)
INSERT INTO QUESTOES VALUES(7,'C02Q02','O que \u00e9 um cont\u00eainer Docker?','Uma m\u00e1quina virtual','Um pacote de software que inclui tudo necess\u00e1rio para executar um aplicativo','Um servi\u00e7o na nuvem','Um banco de dados','B','Docker',1)
INSERT INTO QUESTOES VALUES(8,'C02Q03','Qual comando Docker \u00e9 usado para listar todos os cont\u00eaineres em execu\u00e7\u00e3o?','docker ps','docker ls','docker list','docker run','A','Docker',1)
INSERT INTO QUESTOES VALUES(9,'C02Q04','Qual comando Docker \u00e9 usado para obter uma imagem do Docker Hub?','docker download','docker get','docker pull','docker push','C','Docker',1)
